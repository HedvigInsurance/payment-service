schema {
  query: Query
}

type Query {
  getBankAccountInfo: BankAccount
  getChargeDate: LocalDate
}

type BankAccount {
  bankName: String!
  descriptor: String!
  directDebitStatus: DirectDebitStatus
}

enum DirectDebitStatus {
  CONNECTED
  PENDING
  FAILED
}

# A String-representation of `java.time.LocalDate`, ex:  `"2018-09-26"`
scalar LocalDate
